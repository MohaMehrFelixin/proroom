# Domain for the application
DOMAIN=proroom.local

# Server public IP (for WebRTC/TURN)
SERVER_PUBLIC_IP=YOUR_SERVER_IP

# Database
DB_PASSWORD=change-this-strong-password-32chars

# Redis
REDIS_PASSWORD=change-this-redis-password

# JWT (generate with: openssl rand -hex 64)
JWT_SECRET=change-this-to-a-random-64-char-string-minimum-security
JWT_REFRESH_SECRET=change-this-to-another-random-64-char-string

# MinIO (S3-compatible file storage)
MINIO_ACCESS_KEY=proroom-admin
MINIO_SECRET_KEY=change-this-minio-secret-key-min32chars

# TURN server shared secret (must match coturn config)
TURN_SECRET=change-this-turn-secret-must-be-strong
